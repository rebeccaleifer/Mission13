@{
}
@model IEnumerable<Team>

<a asp-controller="Home" asp-action="Index"></a>

@foreach(Team t in Model)
{
    <a asp-controller="Home" asp-action="Index" asp-route-teamid="@t.TeamID" 
       class="btn @(ViewBag.SelectedTeam == t ? "btn-primary" : "btn-outline-secondary")"
       style="margin-top: 7px; margin-right: 4px; width:120px;">@t.TeamName</a><br />
}